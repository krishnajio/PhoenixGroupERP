							<xs:element name="Pay_mode" type="xs:string" />
							<xs:element name="Cheque_no" type="xs:string" />
							<xs:element name="Narration" type="xs:string" />
							<xs:element name="Group_name" type="xs:string" />
							<xs:element name="Sub_group_name" type="xs:string" />
							<xs:element name="Ch_issue_date" type="xs:dateTime" minOccurs="0" />
							<xs:element name="Ch_date" type="xs:dateTime" minOccurs="0" />
							<xs:element name="balance" type="xs:double" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>